{
  "name": "@farm/codegen",
  "version": "0.1.0",
  "description": "FARM Framework code generation tools for type-safe full-stack development",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "farm-codegen": "dist/cli/index.js"
  },
  "scripts": {
    "build:bundle": "tsup",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/cli/index.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "axios": "^1.9.0",
    "chalk": "^5.3.0",
    "change-case": "^5.4.4",
    "chokidar": "^3.5.3",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "openapi-types": "^12.1.3",
    "ora": "^7.0.1",
    "@farm/types": "workspace:*",
    "@farm/cli": "workspace:*",
    "glob": "^10.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.4",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.14.196",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "vitest": "^3.2.2",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {
    "fastapi": "*",
    "uvicorn": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/farm-stack/framework.git",
    "directory": "tools/codegen"
  },
  "keywords": [
    "farm",
    "fastapi",
    "react",
    "typescript",
    "openapi",
    "code-generation",
    "full-stack",
    "ai"
  ],
  "author": "FARM Stack Team",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
