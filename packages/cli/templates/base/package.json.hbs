{
  "name": "{{projectNameKebab}}",
  "version": "0.1.0",
  "description": "{{projectName}} - Generated by FARM Stack Framework",
  "private": true,
  "type": "module",  {{#unless (eq template "api-only")}}
  "workspaces": ["apps/*"],
  {{/unless}}
  "scripts": {
    "dev": "farm dev",
    "dev:frontend": "farm dev --frontend-only",
    "dev:backend": "farm dev --backend-only",
    "dev:verbose": "farm dev --verbose",
    "build": "farm build",
    "build:frontend": "farm build --frontend-only",
    "build:backend": "farm build --backend-only",
    "start": "farm start",
    "generate": "farm generate",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-sync": "farm generate --types-only"{{#if docker}},
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:build": "docker-compose build"{{/if}}{{#if git}},
    "prepare": "husky install"{{/if}}
  },
  "dependencies": {
    "@farm-framework/core": "^0.1.0",
    "@farm-framework/types": "^0.1.0"{{#unless (eq template "api-only")}},
    "@farm-framework/api-client": "^0.1.0",
    "@farm-framework/ui-components": "^0.1.0"{{/unless}}{{#if_feature "ai"}},    "@farm-framework/ai": "^0.1.0",
    "openai": "^5.3.0"{{/if_feature}}{{#if_feature "auth"}},
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^3.0.2",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/bcryptjs": "^2.4.0"{{/if_feature}}{{#if_feature "realtime"}},
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1"{{/if_feature}}{{#if_feature "storage"}},
    "multer": "^2.0.1",
    "aws-sdk": "^2.1692.0",
    "@types/multer": "^1.4.0"{{/if_feature}}{{#if_feature "payments"}},
    "stripe": "^18.2.1"{{/if_feature}}{{#if_feature "email"}},
    "nodemailer": "^7.0.3",
    "@types/nodemailer": "^6.4.0"{{/if_feature}}{{#if_feature "search"}},
    "elasticsearch": "^16.7.3"{{/if_feature}}{{#if_feature "analytics"}},
    "mixpanel": "^0.18.1"{{/if_feature}}
  },
  "devDependencies": {
    "@farm-framework/dev-server": "workspace:*",
    "@farm-framework/type-sync": "^0.1.0",
    "@farm-framework/observability": "^0.1.0",    "@types/node": "^22.0.0",
    "typescript": "^5.8.3",
    "vitest": "^3.2.3",
    "@vitest/ui": "^3.2.3",
    "eslint": "^9.28.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "prettier": "^3.5.3"{{#unless (eq template "api-only")}},
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0"{{/unless}}{{#unless (eq template "api-only")}},
    "concurrently": "^9.1.2"{{/unless}}{{#if git}},
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0"{{/if}}
  },
  "keywords": [
    "farm",
    "farm-framework",
    "fullstack",
    "typescript",
    {{#unless (eq template "api-only")}}
    "react",
    "monorepo",
    {{/unless}}
    "fastapi",
    "python",
    "{{database}}",
    "ollama",
    "development",
    "framework"{{#if_feature "ai"}},
    "ai",
    "machine-learning",
    "artificial-intelligence",
    "llm"{{/if_feature}}{{#if_feature "auth"}},
    "authentication",
    "authorization",
    "jwt"{{/if_feature}}{{#if_feature "realtime"}},
    "realtime",
    "websockets",
    "socketio"{{/if_feature}}{{#if_feature "storage"}},
    "storage",
    "file-upload",
    "aws"{{/if_feature}}{{#if_feature "payments"}},
    "payments",
    "stripe",
    "ecommerce"{{/if_feature}}{{#if_feature "email"}},
    "email",
    "notifications"{{/if_feature}}{{#if_feature "search"}},
    "search",
    "elasticsearch"{{/if_feature}}{{#if_feature "analytics"}},
    "analytics",
    "tracking"{{/if_feature}}
  ],
  "author": "{{author}}",
  "license": "MIT",  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.12.1"
}